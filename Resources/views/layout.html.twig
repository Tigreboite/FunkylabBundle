{% block flash_message %}
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                {% set flashes = app.session.flashbag.all() %}
                {% set css = {'error': 'warning', 'success': 'success'} %}

                {% if flashes | length > 0 %}
                    {% for type, messages in flashes %}
                        {% for message in messages %}
                            <div class="alert alert-{{ css[type] }}">{{ message }}</div>
                        {% endfor %}
                    {% endfor %}
                {% endif %}
            </div>
        </div>
    </div>
{% endblock flash_message %}


<!-- header logo: style can be found in header.less -->
<header class="header">
    <a href="{{ path('funkylab_home') }}" class="logo">Funkylab</a>
    <!-- Header Navbar: style can be found in header.less -->
    <nav class="navbar navbar-static-top" role="navigation">
        <!-- Sidebar toggle button-->
        <a href="#" class="navbar-btn sidebar-toggle" data-toggle="offcanvas" role="button">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </a>
        <div class="navbar-right">
            <ul class="nav navbar-nav">
                {#
                    {{  include('TigreboiteFunkylabBundle:Helper:message.html.twig', {}) }}
                    {{  include('TigreboiteFunkylabBundle:Helper:notification.html.twig', {}) }}
                    {{  include('TigreboiteFunkylabBundle:Helper:task.html.twig', {}) }}
                #}

                <!-- User Account: style can be found in dropdown.less -->
                <li class="dropdown user user-menu">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <i class="glyphicon glyphicon-user"></i>
                        <span>{{ app.user.username }} <i class="caret"></i></span>
                    </a>
                    <ul class="dropdown-menu">
                        <!-- User image -->
                        <li class="user-header bg-light-blue">
                            <img src="{{ asset('bundles/tigreboitefunkylab/images/user.png') }}" class="img-circle" alt="User Image" />
                            <p>
                                {{ app.user.username }}
                            </p>
                        </li>
                        <!-- Menu Footer-->
                        <li class="user-footer">
                            <div class="pull-left">
                                <a href="{{ path('admin_user_edit',{'id':app.user.id}) }}" class="btn btn-default btn-flat">Profile</a>
                            </div>
                            <div class="pull-right">
                                <a href="{{ path('tigreboite_funkylab_admin_logout') }}" class="btn btn-default btn-flat">Sign out</a>
                            </div>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
    </nav>
</header>
