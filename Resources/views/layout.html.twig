<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Tigreboite/Funlylab | {{ knp_menu_render('breadcrumb', {'template': 'TigreboiteFunkylabBundle:Menu:title.html.twig'}) }}</title>
    {% block head %}{% endblock %}
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
    <!-- bootstrap 3.0.2 -->
    <link href="{{ asset('bundles/adminlte/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">

    <link href="{{ asset('bundles/adminlte/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/adminlte/plugins/datatables/dataTables.bootstrap.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/adminlte/plugins/datepicker/datepicker3.css') }}" rel="stylesheet" type="text/css" />

    <link href="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/css/froala_editor.min.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/css/froala_style.min.css') }}" rel="stylesheet" type="text/css" />

    <!-- CMS style -->
    <link href="{{ asset('bundles/tigreboitefunkylab/vendor/css/smoothness/jquery-ui-1.10.0.custom.min.css') }}" rel="stylesheet" type="text/css" />

    <link href="{{ asset('bundles/adminlte/dist/css/AdminLTE.min.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/adminlte/dist/css/skins/'~ admin_skin ~'.min.css') }}" rel="stylesheet" type="text/css" />

    <link href="{{ asset('bundles/tigreboitefunkylab/css/style.css') }}" rel="stylesheet" type="text/css" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <link rel="stylesheet" href="{{ asset('bundles/lexiktranslation/ng-table/ng-table.min.css') }}">
    <link href="{{ asset('bundles/tigreboitefunkylab/vendor/select2/css/select2.min.css') }}" rel="stylesheet" type="text/css" />
</head>
<body class="hold-transition {{ admin_skin }} sidebar-mini {% if app.request.cookies.has('sidebar_collasped') %}sidebar-collapse{% endif %}">

<div class="wrapper">
    <!-- header logo: style can be found in header.less -->
    <header class="main-header">
        <!-- Logo -->
        <a href="{{ path('funkylab_home') }}" class="logo">
            <span class="logo-mini"><b style="color:white;">F</b><span style="color:black;">LB</span></span>
            <span class="logo-lg"><b><span style="color:white;">Funky</span><span style="color:black;">lab</span></b></span>
        </a>

        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" role="navigation">
            <!-- Sidebar toggle button-->
            <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                <span class="sr-only">Toggle navigation</span>
            </a>
            <div class="navbar-custom-menu">
                <ul class="nav navbar-nav">

                    <!-- User Account: style can be found in dropdown.less -->
                    <li class="dropdown user user-menu">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <i class="glyphicon glyphicon-user"></i>
                            <span>{{ app.user.firstname }} {{ app.user.lastname }}<i class="caret"></i></span>
                        </a>
                        <ul class="dropdown-menu">
                            <!-- User image -->
                            <li class="user-header">
                                {% if app.user.image %}
                                    <img src="{{ asset(app.user.image) }}" class="img-circle"  />
                                {% else %}
                                    <img src="{{ asset('bundles/tigreboitefunkylab/images/user.png') }}" class="img-circle"  />
                                {% endif %}
                                <p>
                                    {{ app.user.firstname }} {{ app.user.lastname }}
                                </p>
                            </li>
                            <!-- Menu Footer-->
                            <li class="user-footer">
                                <div class="pull-left">
                                    &nbsp;
                                </div>
                                <div class="pull-right">
                                    <a href="{{ path('tigreboite_funkylab_admin_logout') }}" class="btn btn-default btn-flat">{{ "funkylab.logout"|trans }}</a>
                                </div>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <aside class="main-sidebar">
        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">
            <!-- Sidebar user panel -->
            <div class="user-panel">
                <div class="pull-left image">
                    {% if app.user.image %}
                        <img src="{{ asset(app.user.image) }}" class="img-circle" />
                    {% else %}
                        <img src="{{ asset('bundles/tigreboitefunkylab/images/user.png') }}" class="img-circle" />
                    {% endif %}
                </div>
                <div class="pull-left info">
                    <br/>
                    <p>{{ app.user.firstname }} {{ app.user.lastname }}</p>
                </div>
            </div>
            <!-- sidebar menu: : style can be found in sidebar.less -->
            {{ knp_menu_render('main_menu', {'currentClass': 'active', 'template': 'TigreboiteFunkylabBundle:Menu:sidebar-menu.html.twig'}) }}
        </section>
        <!-- /.sidebar -->
    </aside>
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                {{ knp_menu_render('breadcrumb', {'template': 'TigreboiteFunkylabBundle:Menu:breadcrumb-left.html.twig'}) }}
            </h1>
            {{ knp_menu_render('breadcrumb', {'currentClass': 'active', 'template': 'TigreboiteFunkylabBundle:Menu:breadcrumb.html.twig'}) }}
        </section>

        <section class="content">
            <div class="row">
                <div class="col-xs-12">
                    {% block action %}{% endblock %}
                    <p>&nbsp;</p>
                    {% block content %}{% endblock %}
                </div>
            </div>
        </section><!-- /.content -->
    </div>

    <script src="{{ asset('bundles/adminlte/plugins/jQuery/jQuery-2.1.4.min.js') }}"></script>
    <script src="{{ asset('bundles/adminlte/plugins/jQueryUI/jquery-ui.min.js') }}" type="text/javascript"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>

    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
    <!-- Bootstrap -->
    <script src="{{ asset('bundles/adminlte/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
    <!-- AdminLTE App -->
    <script type="text/javascript">
        var AdminLTEOptions = {
            animationSpeed: 500,
            sidebarPushMenu: false
        };
    </script>
    <script src="{{ asset('bundles/adminlte/dist/js/app.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/adminlte/plugins/datatables/jquery.dataTables.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/adminlte/plugins/datatables/dataTables.bootstrap.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/jquery.confirm.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/jquery.fileupload.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/jquery.fileupload-jquery-ui.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/jquery.cookie.js') }}" type="text/javascript"></script>

    <!-- Bootstrap WYSIHTML5 -->
    <script src="{{ asset('bundles/adminlte/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js') }}" type="text/javascript"></script>
    <!-- Slimscroll -->
    <script src="{{ asset('bundles/adminlte/plugins/slimScroll/jquery.slimscroll.min.js') }}" type="text/javascript"></script>
    <!-- FastClick -->
    <script src="{{ asset('bundles/adminlte/plugins/fastclick/fastclick.min.js') }}" type="text/javascript"></script>
    {#<script src="{{ asset('bundles/adminlte/plugins/datepicker/bootstrap-datepicker.js') }}" type="text/javascript"></script>#}

    <!-- Froala -->
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/js/froala_fix.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/js/froala_editor.min.js') }}" type="text/javascript"></script>
    <!--[if lt IE 9]>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/js/froala_editor_ie8.min.js') }}" type="text/javascript"></script>
    <![endif]-->
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/js/plugins/tables.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/js/plugins/urls.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/js/plugins/lists.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/js/plugins/colors.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/js/plugins/font_family.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/js/plugins/font_size.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/js/plugins/block_styles.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/js/plugins/video.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/js/plugins/char_counter.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/froala_editor/js/plugins/media_manager.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/tigreboitefunkylab/vendor/select2/js/select2.min.js') }}" type="text/javascript"></script>
    <script type="text/javascript">
        $.Editable.DEFAULTS.key = "{{ froala_editor }}";
    </script>

    <script src="{{ asset('bundles/tigreboitefunkylab/js/main.js') }}" type="text/javascript"></script>

    {% block js %}{% endblock %}

    {% block javascript_footer %}
        <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.13/angular.min.js"></script>
        <script src="{{ asset('bundles/lexiktranslation/ng-table/ng-table.min.js') }}"></script>
    {% endblock %}

</div>
</body>
</html>
